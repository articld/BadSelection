{
  "name": "Bad Selection",
  "author": "Articld",
  "version": "0.6.0",
  "keywords": [
    "pebble-watchface"
  ],
  "private": false,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "capabilities": [
    "configurable"
  ],
  "pebble": {
    "displayName": "Bad Selection",
    "uuid": "77e68bfb-2c15-4652-8e9a-fd658da49f8f",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "diorite"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "AccentColor",
      "BGColor",
      "TimeColor",
      "TextGridColor",
      "TextGridAnimation",
      "TimeBoxPosition",
      "Date"
    ],
    "resources": {
      "media": [
        {
          "type": "font",
          "name": "FONT_FEATURE_MONO_BLACK_35",
          "file": "fonts/featuremono-black.ttf",
          "characterRegex": "[0-9:APM ]"
        },
        {
          "type": "font",
          "name": "FONT_FEATURE_MONO_BOLD_20",
          "file": "fonts/featuremono-bold.ttf",
          "characterRegex": "[/0-9]"
        },
        {
          "type": "font",
          "name": "FONT_FEATURE_MONO_REGULAR_20",
          "file": "fonts/featuremono-regular.ttf",
          "characterRegex": "[a-z]"
        }
      ]
    }
  }
}
